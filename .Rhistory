library("data.table")
setwd("E:\\Coursera\\Exploratory Data Analysis\\exdata_data_household_power_consumption")
powerDT <- data.table::fread(input = "household_power_consumption.txt", na.strings = "?")
powerDT[, Date := lapply(.SD, as.Date, "%d/%m/%Y"), .SDcols = c("Date")]
powerDT <- powerDT[(Date >= "2007-02-01") & (Date <= "2007-02-02")]
globalActivePower <- as.numeric(powerDT$Global_active_power)
powerDT[, dateTime := as.POSIXct(paste(Date, Time), format = "%d/%m/%Y %H:%M:%S")]
plot(x = powerDT[,dateTime], y = powerDT[, globalActivePower], xlab = "", ylab = "Global Active Power (kilowatts)")
plot(x = powerDT[,dateTime], y = powerDT[, Global_active_power], xlab = "", ylab = "Global Active Power (kilowatts)")
